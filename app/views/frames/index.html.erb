<div id="frames">
  <br>
  <%= render partial: "frames/more/load_prev", locals: { pagy: @pagy, word: @items["word"] } %>
  <div id="list">
    <%= render partial: "frames/more/list", locals: { frames: @frames } %>
  </div>
  <%= render partial: "frames/more/load_next", locals: { pagy: @pagy, word: @items["word"] } %>

  <% if false %><div class="flex justify-center">
    <div class="mx-auto rounded-[5px] pt-2">
      <%== pagy_nav(@pagy) %>
    </div>
  </div><% end %>
</div>
